# ===================================
# Local AI .env template (change for your setup)
# ===================================

# --- Postgres Credentials ---
POSTGRES_USER=root
POSTGRES_PASSWORD=password
POSTGRES_DB=n8n

# --- Cloudflare Tunnel (to access services from the web ---
TUNNEL_TOKEN= [paste token here]

# --- OpenAI / Ollama / Modelli ---
OPENAI_API_KEY= [paste API key here]
OPENAI_BASE_URL=https://api.openai.com/v1
MODEL_NAME=gpt-4o-mini
MODEL_EMBEDDING_NAME=text-embedding-3-small
OLLAMA_BASE_URL=http://host.docker.internal:11434
OLLAMA_HOST=host.docker.internal/11434
MODEL_OLLAMA_NAME=gpt-oss:20b
MODEL_OLLAMA_EMBEDDING_NAME=embeddinggemma:latest

# --- n8n ---
N8N_ENCRYPTION_KEY=super-secret-key
N8N_USER_MANAGEMENT_JWT_SECRET=even-more-secret
N8N_COMMUNITY_PACKAGES_ALLOW_TOOL_USAGE=true
WEBHOOK_URL= http://localhost:5678
